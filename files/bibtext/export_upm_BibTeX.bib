       booktitle = {Proceedings of the 19th International Conference on Software Engineering and Knowledge Engineering, SEKE 2007},
         address = {Ilinois, Usa},
           title = {Methodology for Reusing Human Resources Management Standards},
       publisher = {Knowledge Systems Institute},
            year = {2007},
